From: Daniel Micay <danielmicay@gmail.com>
Date: Sun, 4 Dec 2016 22:30:01 -0500
Subject: [PATCH 13/16] CopperheadOS uses the stable channel

---
 chrome/common/channel_info_android.cc | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/chrome/common/channel_info_android.cc b/chrome/common/channel_info_android.cc
index 11d64ed126ca..5e5918808863 100644
--- a/chrome/common/channel_info_android.cc
+++ b/chrome/common/channel_info_android.cc
@@ -37,7 +37,7 @@ version_info::Channel GetChannel() {
   if (!strcmp(bi->package_name(), "com.chrome.canary"))
     return version_info::Channel::CANARY;
 
-  return version_info::Channel::UNKNOWN;
+  return version_info::Channel::STABLE;
 }
 
 }  // namespace chrome
