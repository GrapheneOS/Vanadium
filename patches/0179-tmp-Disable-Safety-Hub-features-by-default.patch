From: quh4gko8 <88831734+quh4gko8@users.noreply.github.com>
Date: Sat, 30 Nov 2024 07:07:47 +0000
Subject: [PATCH] tmp: Disable Safety Hub features by default

---
 chrome/common/chrome_features.cc    | 8 ++++----
 components/safety_check/features.cc | 2 +-
 2 files changed, 5 insertions(+), 5 deletions(-)

diff --git a/chrome/common/chrome_features.cc b/chrome/common/chrome_features.cc
index 237bcca5b6f0e..406d0fc3030d4 100644
--- a/chrome/common/chrome_features.cc
+++ b/chrome/common/chrome_features.cc
@@ -1192,7 +1192,7 @@ BASE_FEATURE(kRecordWebAppDebugInfo, base::FEATURE_DISABLED_BY_DEFAULT);
 // Enables notification permission revocation for abusive origins.
 BASE_FEATURE(kAbusiveNotificationPermissionRevocation,
              "AbusiveOriginNotificationPermissionRevocation",
-             base::FEATURE_ENABLED_BY_DEFAULT);
+             base::FEATURE_DISABLED_BY_DEFAULT);
 
 #if BUILDFLAG(IS_CHROMEOS)
 // Enables permanent removal of Legacy Supervised Users on startup.
@@ -1212,7 +1212,7 @@ BASE_FEATURE(kSafetyHubExtensionsOffStoreTrigger,
              base::FEATURE_ENABLED_BY_DEFAULT);
 #endif
 
-BASE_FEATURE(kSafetyHubThreeDotDetails, base::FEATURE_ENABLED_BY_DEFAULT);
+BASE_FEATURE(kSafetyHubThreeDotDetails, base::FEATURE_DISABLED_BY_DEFAULT);
 
 BASE_FEATURE(kSafetyHubDisruptiveNotificationRevocation,
              base::FEATURE_DISABLED_BY_DEFAULT);
@@ -1279,10 +1279,10 @@ constexpr base::FeatureParam<int>
 
 #if BUILDFLAG(IS_ANDROID)
 // Enables Safety Hub card in magic stack.
-BASE_FEATURE(kSafetyHubMagicStack, base::FEATURE_ENABLED_BY_DEFAULT);
+BASE_FEATURE(kSafetyHubMagicStack, base::FEATURE_DISABLED_BY_DEFAULT);
 
 // Enables Safety Hub followup work.
-BASE_FEATURE(kSafetyHubFollowup, base::FEATURE_ENABLED_BY_DEFAULT);
+BASE_FEATURE(kSafetyHubFollowup, base::FEATURE_DISABLED_BY_DEFAULT);
 
 // Enables Safety Hub organic HaTS survey on Android.
 BASE_FEATURE(kSafetyHubAndroidOrganicSurvey, base::FEATURE_DISABLED_BY_DEFAULT);
diff --git a/components/safety_check/features.cc b/components/safety_check/features.cc
index 653404436130b..a0403e7d1cd40 100644
--- a/components/safety_check/features.cc
+++ b/components/safety_check/features.cc
@@ -8,7 +8,7 @@
 
 namespace safety_check::features {
 
-BASE_FEATURE(kSafetyHub, base::FEATURE_ENABLED_BY_DEFAULT);
+BASE_FEATURE(kSafetyHub, base::FEATURE_DISABLED_BY_DEFAULT);
 
 // Time between automated runs of the password check.
 const base::FeatureParam<base::TimeDelta> kBackgroundPasswordCheckInterval{
